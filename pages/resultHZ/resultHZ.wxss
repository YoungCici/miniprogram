page{
  width: 100%;
  height: 100%;
  position: relative;
}
.resultHZ-container{
  width: 100%;
  height: 100%;
  background: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/resultHZBG.png') no-repeat center;
  background-size: 100% 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: flex-start;
  box-sizing: border-box;
}
.resultHZ-person{
  background: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/resultHZBox.png')no-repeat center;
  background-size: 100% 100%;
  width: 625rpx;
  height: 853rpx;
  margin-top: 15rpx;
}
.resultHZ-title{
  width: 100%;
  height: 81rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
  margin-top: 66rpx;
}
.person-title{
  width: 81rpx;
  height: 81rpx;
  border-radius: 50%;
  background: #ff9696;
}
.person-item{
  color: #595959;
  font-weight: bold;
  font-size: 27rpx;
  margin-left: 6rpx;
}
.page-hz-title, .page-hz-subTitle, .page-hz-score{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 34rpx;
  color: #615758;
  font-weight: bold;
  text-align: center;
  margin-top: 40rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
 .page-hz-subTitle{
   margin-top: 40rpx;
 }
.sw-word, .sj-word{
  font-size: 40rpx;
  font-weight: bold;
  color: #373737;
}
.sw-word{
  margin-left: 20rpx;
}
.sj-word{
  margin-left: 80rpx;
}
.page-hz-score{
  margin-top: 50rpx;
}
.sw-score, .sj-score{
  font-size: 75rpx;
  font-weight: bold;
  color: #ff3145;
}
.sw-score{
  margin-left: 10rpx;
}
.sj-score{
  margin-left: 120rpx;
}
.page-hz-content{
  width: 430rpx;
  margin-top: 50rpx;
  margin-left: 109rpx;
}
.content-title{
  font-size: 40rpx;
  color: #595959;
  font-weight: bold;
}
.content-word{
  text-indent: 35rpx;
  margin-left: 15rpx;
  line-height: 40rpx;
  font-weight: bold;
  color: #595959;
  font-size: 32rpx;
}
.resultHZ-btn1{
  width: 100%;
  height: auto;
  margin-top: 25rpx;
  margin-bottom: 49rpx;
  display: flex;
  justify-content: center;
}
.resultHZ-btn1 > button::after{
  border: none;
}
.restart{
  background: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/restartBtn.png') no-repeat center;
  background-size: 100% 100%;
  width: 230rpx;
  height: 100rpx;
  margin-left: 123rpx;
}
.to-share{
  background: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/showResult.png') no-repeat center;
  background-size: 100% 100%;
  width: 230rpx;
  height: 100rpx;
  margin-right: 123rpx;
}
.resultHZ-btn2{
  width: 499rpx;
  height: 129rpx;
  background: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/saveImg.png') no-repeat center;
  background-size: 100% 100%;
}


.canvasBox{
  position: absolute;
  top: 0;
  left:0; 
  z-index: 999999;
}
.btnsave{
  width: 200rpx;
  height: 30px;
  margin: auto;
  background-color: #FF8C00;
}
.btnsave-btn{
  width: 100%;
  height: 100%;
  background: transparent;
}

/* 成功tips */
.wx-mask{
  position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.65);
}
.wx-dialog{
    position: fixed;
    z-index: 5000;
    width: 80%;
    max-width: 600rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #FFFFFF;
    text-align: center;
    border-radius: 20rpx;
    /* overflow: hidden; */
}
.wx-dialog-title{
    font-size: 18px;
    padding: 15px 15px 5px;
}
.wx-dialog-content{
    padding: 40px 15px 10px;
    /* min-height: 280rpx; */
    /* height: 280rpx; */
    font-size: 16px;
    line-height: 1.3;
    word-wrap: break-word;
    word-break: break-all;
    color: #999999;
    position: relative;
    box-sizing:border-box;
}
.tipsSign{
  width: 170rpx;
  height: 245rpx;
  background-image: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/okTip.png');
  background-size: contain;
  background-repeat: no-repeat;
  position: absolute;
  top: -137rpx;
  right: 20rpx;
}
.textTips{
  font-family: PingFang-SC-Heavy;
	font-size: 36rpx;
	line-height: 70rpx;
	color: #ff8f32;
  word-wrap: break-word; 
  word-break: normal; 
  width: 375rpx;
  display: block;
  text-align: center;
  padding-left: 60rpx;
  font-weight: 600;
}

.wx-dialog-footer{
    display: flex;
    align-items: center;
    /* position: relative; */
    line-height: 45px;
    font-size: 17px;
    position:absolute;
    left:116rpx;
    overflow:hidde;
}

.wx-dialog-btn{
    display: block;
    /* -webkit-flex: 1;
    flex: 1; */
    -webkit-tap-highlight-color: rgba(0, 0, 0, 0);
    /* position: relative; */
}
.znawei{
  height: 58rpx;
}
.wx-next-btn{
  width: 341rpx;
	height: 80rpx;
  background-image: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/zhidao.png');
