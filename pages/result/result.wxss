/* 成功tips */
.wx-mask{
  position: fixed;
    z-index: 1000;
    top: 0;
    right: 0;
    left: 0;
    bottom: 0;
    background: rgba(0, 0, 0, 0.65);
}
.wx-dialog{
    position: fixed;
    z-index: 5000;
    width: 80%;
    max-width: 600rpx;
    top: 50%;
    left: 50%;
    -webkit-transform: translate(-50%, -50%);
    transform: translate(-50%, -50%);
    background-color: #FFFFFF;
    text-align: center;
    border-radius: 20rpx;
    /* overflow: hidden; */
}
.wx-dialog-title{
    font-size: 18px;
    padding: 15px 15px 5px;
}
.wx-dialog-content{
    padding: 40px 15px 10px;
    /* min-height: 280rpx; */
    /* height: 280rpx; */
    font-size: 16px;
    line-height: 1.3;
    word-wrap: break-word;
    word-break: break-all;
    color: #999999;
    position: relative;
    box-sizing:border-box;
}
.tipsSign{
  width: 170rpx;
  height: 245rpx;
  background-image: url('https://yhzj.oss-cn-shanghai.aliyuncs.com/xiaochengxu/okTip.png');
  background-size: contain;
  background-repeat: no-repeat;
  position: absolute;
  top: -137rpx;
  right: 20rpx;
}
.textTips{
  font-family: PingFang-SC-Heavy;
	font-size: 36rpx;
	line-height: 70rpx;
	color: #ff8f32;
  word-wrap: break-word; 
  word-break: normal; 
  width: 375rpx;
